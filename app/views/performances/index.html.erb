<% provide(:title, "Performances for #{@contest.name}") %>

<table>
  <thead>
    <tr>
      <th><%= Performance.human_attribute_name(:category) %></th>
    </tr>
  </thead>
  <tbody>
    <% @performances.each do |performance| %>
    <tr>
      <td><%= link_to performance.category.name, performance_path(performance) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
