<% provide(:title, "Performances for #{@contest.name}") %>

<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @performances.each do |performance| %>
    <tr>
      <td><%= link_to "Performance #{performance.id}", performance_path(performance) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
